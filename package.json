{
  "name": "@iad-os/fantocci",
  "version": "0.4.0",
  "description": "Http Testing Tool",
  "bin": {
    "@iad-os/fantocci": "./dist/.bin/cli.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx watch --inspect src/index.ts | pino-pretty-compact",
    "update-schema": "tsx watch --inspect src/index.ts --schema",
    "start": "tsx src/index.ts  | pino-pretty-compact",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint",
    "clean": "rimraf dist",
    "build": "rimraf ./dist && tsc  --incremental false"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    }
  },
  "keywords": [
    "http",
    "testing",
    "httpbin"
  ],
  "files": [
    "dist"
  ],
  "contributors": [
    {
      "name": "Daniele Fiungo",
      "email": "daniele.fiungo@iad2.it"
    },
    {
      "name": "Irene La Bollita",
      "email": "irene.labollita@iad2.it"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.15.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@ghii/ghii-es": "^1.3.1",
    "@ghii/yaml-loader": "^0.0.7",
    "@scalar/fastify-api-reference": "^1.24.68",
    "@sinclair/typebox": "^0.32.35",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "jwt-decode": "^4.0.0",
    "lodash.omit": "^4.5.0",
    "minimist": "^1.2.8",
    "mkcert": "^3.2.0",
    "nanoid": "^5.0.7",
    "openid-client": "^5.6.5",
    "tslib": "^2.6.3",
    "type-fest": "^4.23.0"
  },
  "devDependencies": {
    "@mgcrea/pino-pretty-compact": "^1.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/http-errors": "^2.0.4",
    "@types/lodash.omit": "^4.5.9",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.14.14",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "optionalDependencies": {
    "@scalar/api-reference": "^1.24.68"
  }
}
